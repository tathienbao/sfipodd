{
  "version": 2,
  "builds": [
    {
      "src": "api/download.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/download.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "/index.html"
    }
  ]
}